h1.page-header.clearfix
	.pull-right
		button.btn.btn-sm.btn-primary(
			data-toggle="dropdown",
			aria-expanded="false",
			disabled="true"
		)
			| target revision: 
			span= this.props.project.scm ? this.props.project.scm.rev : ''
		| 
		if this.props.project.name
			button.btn.btn-sm.btn-success(onClick=this.onBuildProject)
				i.fa.fa-fw.fa-play
				| 
				span Build
	div
		Scm(scm=this.props.project.scm ? this.props.project.scm.type : '')
		span= this.props.project.name
